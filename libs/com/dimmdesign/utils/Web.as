package com.dimmdesign.utils {		import flash.external.ExternalInterface;	import flash.net.URLRequest;	import flash.net.navigateToURL;		public class Web {				public static function getURL (url:String, window:String = "_blank"):void {						var req:URLRequest = new URLRequest (url);						try {				navigateToURL(req, window);			}			catch (e:Error) {				trace("Navigate to URL failed", e.message);				throw new Error ("Navigate to URL failed");			}							}				public static function openPopup(__url:String, __width:int = 600, __height:int = 400, __name:String = "_blank"): void {						var js:XML;			js = <script><![CDATA[				function(__url, __width, __height, __name) {										var wx = (screen.width - __width)/2;					var wy = (screen.height - __height)/2 - 142;					var HTMLUtils_newWindow = window.open(__url, __name, "top="+wy+",left="+wx+",width="+__width+",height="+__height);					if (HTMLUtils_newWindow.focus) HTMLUtils_newWindow.focus();								}			]]></script>;						var __onClosedString:String = "";						if (!ExternalInterface.available) {				trace ("No ExternalInterface available!");				return;			}						ExternalInterface.call(js, __url, __width, __height, __name);		}			}}